{
  "functions": {
    "api/extract.py": {
      "runtime": "python3.9"
    }
  },
  "builds": [
    {
      "src": "pci-extractor-web/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "api/extract.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1.py"
    },
    {
      "src": "/(.*)",
      "dest": "/pci-extractor-web/$1"
    }
  ]
}