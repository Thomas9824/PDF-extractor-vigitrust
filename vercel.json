{
  "version": 2,
  "functions": {
    "api/extract.py": {
      "runtime": "python3.9"
    }
  },
  "rewrites": [
    {
      "source": "/api/extract",
      "destination": "/api/extract"
    },
    {
      "source": "/((?!api).*)",
      "destination": "/pci-extractor-web/$1"
    }
  ],
  "builds": [
    {
      "src": "pci-extractor-web/package.json",
      "use": "@vercel/next"
    }
  ]
}