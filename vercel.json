{
  "builds": [
    {
      "src": "pci-extractor-web/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "api/extract.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/extract",
      "dest": "/api/extract.py"
    },
    {
      "src": "/(.*)",
      "dest": "/pci-extractor-web/$1"
    }
  ]
}